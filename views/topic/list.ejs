<% topics.forEach(function(topic, index) {
        if (topic) { %>
<div class="row-fluid">
    <div class="span12">
        <h4><a href="/topic/<%= topic._id %>"><%= topic.title %></a></h4>

        <p>
            <small><%= topic.create_at %></small>
        </p>
        <p><%= topic.content %></p>
    </div>
</div><!-- end row -->
<% } %>
<% }) %>
</div><!-- end row -->